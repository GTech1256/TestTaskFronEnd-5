{
  "name": "express-rest-es2017-boilerplate",
  "version": "1.2.2",
  "description": "Express boilerplate for building RESTful APIs",
  "author": "Roman Bakakin <pomah156@gmail.com>",
  "main": "src/server/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "precommit": "yarn lint",
    "prestart": "yarn docs",
    "start": "cross-env NODE_ENV=production pm2 start ./src/server/index.js",
    "dev:server": "nodemon ./src/server/index.js",
    "lint": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --recursive src/server/api/tests",
    "test:unit": "cross-env NODE_ENV=test mocha src/server.api/tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 src/server/api/tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch src/server/api/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "opn coverage/index.html",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags",
    "deploy": "sh ./deploy.sh",
    "docs": "apidoc -i src/server -o docs",
    "postdocs": "opn docs/index.html",
    "build": "rollup -c rollup.config.js && rollup -c rollup.config.IE.js",
    "autobuild": "rollup -c -w",
    "dev:front": "run-p start:dev autobuild",
    "start:front": "sirv public --single",
    "start:dev": "sirv public --single --dev"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:GTech1256/TestTaskFronEnd-5.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es2017",
    "mocha",
    "nyc",
    "eslint",
    "Travis CI",
    "coveralls",
    "REST",
    "API",
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.0",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "dotenv": "^8.1.0",
    "dotenv-safe": "^6.0.0",
    "express": "^4.15.2",
    "express-validation": "^1.0.2",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "joi": "^10.4.1",
    "jwt-simple": "0.5.6",
    "lodash": "^4.17.4",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.13",
    "mongoose": "^5.2.17",
    "morgan": "^1.8.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "4.0.0",
    "pm2": "^3.0.0",
    "sirv-cli": "^0.4.4",
    "uuid": "^3.1.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "apidoc": "^0.17.5",
    "autoprefixer": "^9.6.1",
    "axios-mock-adapter": "^1.17.0",
    "babel-plugin-transform-es2015-for-of": "^6.23.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "core-js": "3",
    "coveralls": "^3.0.0",
    "es6-shim": "^0.35.5",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.14.3",
    "iterators-polyfill": "^1.0.2",
    "jest": "^24.9.0",
    "mocha": "^3.3.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.12.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^12.0.2",
    "opn-cli": "^3.1.0",
    "postcss": "^7.0.18",
    "postcss-load-config": "^2.1.0",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-polyfill": "^3.0.0",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-shims": "^1.1.2",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.0.0",
    "supertest": "^4.0.2",
    "svelte": "^3.12.1",
    "svelte-preprocess": "^3.1.1",
    "xhr-mock": "^2.5.0"
  }
}
